<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 	<class name="com.rhsmith.Wrapper.DepartmentWrapper" table="Department">
 	 	<id column="departmentID" name="departmentID" type="java.lang.String">
 	 	</id>
 	 	<property column="departmentName" name="departmentName" type="java.lang.String"/>
<property column="departmentAddress" name="departmentAddress" type="java.lang.String"/>
<property column="country" name="country" type="java.lang.String"/>
<property column="departmentPhone"  name="departmentPhone" type="java.lang.String"/>
<property column="departmentSupervisor" name="departmentSupervisor" type="java.lang.String"/>
 	</class>
</hibernate-mapping>
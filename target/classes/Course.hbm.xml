<hibernate-mapping>
 	<class name="com.rhsmith.Wrapper.CourseWrapper" table="Course">
 	 	<id column="courseID" name="courseID" type="java.lang.String">
 	 	</id>
 <property column="courseName" name="courseName" type="java.lang.String"/>
<property column="courseLimit" name="courseLimit" type="java.lang.String"/>
